<head>
  <title>Bootstrap Example</title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
</head>

<p id="notice"><%= notice %></p>

<div class="container"><h1>Nav Logs</h1></div>

<div class="container">
<table class="table">
  <thead>
    <tr>
      <th>Altitude</th>
      <th>Gps latitude</th>
      <th>Gps longitude</th>
      <th>Drone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nav_logs.each do |nav_log| %>
      <tr>
        <td><%= nav_log.altitude %></td>
        <td><%= nav_log.gps_latitude %></td>
        <td><%= nav_log.gps_longitude %></td>
        <td><%= nav_log.drone %></td>
        <td><%= link_to 'Show', nav_log %></td>
        <td><%= link_to 'Edit', edit_nav_log_path(nav_log) %></td>
        <td><%= link_to 'Destroy', nav_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nav Log', new_nav_log_path %>
</div>

<br>
<hr>

<div class="container">
<h3>Riak Data</h3>
</div>

<div class="container">
<table class="table">
  <thead>
  <tr>
    <th>Drone Name</th>
    <th>Altitude</th>
    <th>GPS Latitude</th>
    <th>GPS Longitude</th>
    <th>Date</th>
    <th>Order ID</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>

  <% @logs.each do |log| %>
    <tr>
      <td>Orion</td>
      <td><%= log["altitude"] %></td>
      <td><%= log["gps_latitude"] %></td>
      <td><%= log["gps_longitude"] %></td>
      <td><%= log["datetime"] %></td>
      <td><%= log["order_id"] %></td>
    </tr>
  <% end %>

  </tbody>
</table>
</div>
