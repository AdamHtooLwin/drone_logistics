<p id="notice"><%= notice %></p>

<h1>Addresses</h1>

<table>
  <thead>
    <tr>
      <th>Street address</th>
      <th>Road</th>
      <th>District</th>
      <th>City</th>
      <th>Gps latitude</th>
      <th>Gps longitude</th>
      <th>Customer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @addresses.each do |address| %>
      <tr>
        <td><%= address.street_address %></td>
        <td><%= address.road %></td>
        <td><%= address.district %></td>
        <td><%= address.city %></td>
        <td><%= address.gps_latitude %></td>
        <td><%= address.gps_longitude %></td>
        <td><%= address.customer %></td>
        <td><%= link_to 'Show', address %></td>
        <td><%= link_to 'Edit', edit_address_path(address) %></td>
        <td><%= link_to 'Destroy', address, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Address', new_address_path %>
